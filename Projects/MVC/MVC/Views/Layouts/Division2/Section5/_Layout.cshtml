<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
	<head>
		<meta charset="utf-8" />
		<title>@ViewBag.Title</title>
		<link href="@Url.Content("~/Styles/Division2.css")" rel="stylesheet" type="text/css" />
		<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
		<script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
	</head>
	<body>
		<div id="idGeneralLayout">
			<Header id="idHeaderRow">
				<div id="idHeaderRowCenter">
					<img id="idLogo" src="@Url.Content("~/Pictures/logoHTKB.jpg")"><br>
					</div>
				</div>
			</Header>
			<Nav id="idNavigationRow">
				<div id="idNavigationBar">
					@Html.ActionLink("Home", "Index", "Main")
					@Html.ActionLink("Web Programming", "Index", "Division1")
					@Html.ActionLink("Private Projects", "Index", "Division2")
					@Html.ActionLink("Downloadable Projects", "Index", "Division3")
				</div>
			</Nav>
			<div id="idCenterRow">
				<div id="idCenterRowLeft">
					<h4>
						Navigation
					</h4>
						@Html.ActionLink("Gynowars", "Index", "Section2_1")<br /><br />
						@Html.ActionLink("Assault", "Project2", "Division2")<br /><br />
						@Html.ActionLink("Mars", "Project3", "Division2")<br /><br />
						@Html.ActionLink("Renley", "Index", "Section2_4")<br /><br />
						@Html.ActionLink("Anatarrea", "Index", "Section2_5")<br /><br />
							@Html.ActionLink("Global", "Index", "Section2_5_1")<br /><br />
							@Html.ActionLink("Grendol", "Index", "Section2_5_2")<br /><br />
							@Html.ActionLink("Utopia", "Index", "Section2_5_3")<br /><br />
							@Html.ActionLink("Elvia", "Index", "Section2_5_4")<br /><br />
						@Html.ActionLink("Truth", "Project6", "Division2")<br /><br />
						@Html.ActionLink("Kingdoms", "Project7", "Division2")<br /><br />
						@Html.ActionLink("Terminal World", "Project8", "Division2")<br /><br />
						@Html.ActionLink("Monster Office Workplace", "Project9", "Division2")<br /><br />
						@Html.ActionLink("Battle Princesses", "Project10", "Division2")<br /><br />
						@Html.ActionLink("Sacred Offerings", "Project11", "Division2")<br /><br />
						@Html.ActionLink("The Way", "Project12", "Division2")<br /><br />
						@Html.ActionLink("Conspiratorium", "Project13", "Division2")<br /><br />
						@Html.ActionLink("Conversion", "Project14", "Division2")<br /><br />
				</div>
				<div id="idCenterRowMain">
					@RenderBody()
				</div>
				<div id="idCenterRowRight">
					<h4>
						Information
					</h4>
					This is written with ASP.NET as a MVC Application.<br /><br />
					Other verisons can be found here:<br />
					@Html.Raw(@Page.Versions)
				</div>
			</div>
			<Footer id="idFooterRow">
				<div id="idFooterMain">
					<div id="idFooterContent">
						© Copyright 2012 All rights reserved<br>
						House That Kamurai Built<br>
					</div>
					<div id="idFooterManagement">
						Website managed by Kamurai.
					</div>
				</div>
			</Footer>
		</div>
	</body>
</html>

