					<a class="navBar" href="../../Division2/Index.cshtml">Private Projects</a>
					<a class="navBar" href="../../Division3/Index.cshtml">Downloadable Projects</a>
				</div>
			</Nav>
			<div id="idCenterRow">
				<div id="idCenterRowLeft">
					<h4>
						Navigation
					</h4>
					<a class="navlinkA" href="../Section1/Index.cshtml">Gynowars</a><br /><br />
						<a class="navlinkB" href="./Project1.cshtml">Gynowars RPG</a><br /><br />
						<a class="navlinkB" href="./Project2.cshtml">Gynowars: Battle Arena CCG</a><br /><br />
						<a class="navlinkB" href="./Project3.cshtml">Gynowars: Crash Ball</a><br /><br />
					<a class="navlinkA" href="../Project2.cshtml">Assault</a><br /><br />
					<a class="navlinkA" href="../Project3.cshtml">Mars</a><br /><br />
					<a class="navlinkA" href="../Section4/Index.cshtml">Renley</a><br /><br />
					<a class="navlinkA" href="../Section5/Index.cshtml">Antarrea</a><br /><br />
					<a class="navlinkA" href="../Section6/Index.cshtml">Editations</a><br /><br />
					<a class="navlinkA" href="../Project7.cshtml">Truth</a><br /><br />
					<a class="navlinkA" href="../Project8.cshtml">Kingdoms</a><br /><br />
					<a class="navlinkA" href="../Project9.cshtml">Terminal World</a><br /><br />
					<a class="navlinkA" href="../Project10.cshtml">Monster Office Workplace</a><br /><br />
					<a class="navlinkA" href="../Project11.cshtml">Battle Princesses</a><br /><br />
					<a class="navlinkA" href="../Project12.cshtml">Sacred Offerings</a><br /><br />
					<a class="navlinkA" href="../Project13.cshtml">The Way</a><br /><br />
					<a class="navlinkA" href="../Project14.cshtml">Conspiratorium</a><br /><br />
					<a class="navlinkA" href="../Project15.cshtml">Conversion</a><br /><br />
				</div>
				<div id="idCenterRowMain">
					<h2>
						@Page.Title
					</h2>
					@RenderBody()
				</div>
				<div id="idCenterRowRight">
					<h4>
						Information
					</h4>
					This is written with ASP.NET as a Web Page Application.<br />
					Other versions can be found here:<br />
					@Html.Raw(@Page.Versions)
				</div>
			</div>
			<Footer id="idFooterRow">
				<div id="idFooterMain">
					<div id="idFooterContent">
						© Copyright 2012 All rights reserved<br>
						House That Kamurai Built<br>
					</div>
					<div id="idFooterManagement">
						Website managed by Kamurai.
					</div>
				</div>
			</Footer>
		</div>
	</body>
</html>