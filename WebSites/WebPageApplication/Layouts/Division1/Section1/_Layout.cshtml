<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head>
    <meta charset="utf-8" />
    <title>HTKB: @Page.Title</title>
    @RenderSection("head", required: false)
    <link href="@Href("~/Styles/Division1.css")" rel="stylesheet" type="text/css" />
    <link runat="server" rel="shortcut icon" href="../../favicon.ico" type="image/x-icon" />
    <link runat="server" rel="icon" href="../../favicon.ico" type="image/ico" />
</head>
<body>
    <div id="idGeneralLayout">
        <Header id="idHeaderRow">
            <div id="idHeaderRowCenter">
                <img id="idLogo" src="../../Pictures/logoHTKB.jpg" /><br>
            </div>
        </Header>
        <Nav id="idNavigationRow">
            <div id="idNavigationBar">
                <a class="navBar" href="../../Index.cshtml">Home</a>
                <a class="navBar" href="../../Division1/Index.cshtml">Web Programming</a>
                <a class="navBar" href="../../Division2/Index.cshtml">Private Projects</a>
                <a class="navBar" href="../../Division3/Index.cshtml">Downloadable Projects</a>
            </div>
        </Nav>
        <div id="idCenterRow">
            <div id="idCenterRowLeft">
                <h4>
                    Navigation
                </h4>
                <a class="navlinkA" href="../Section1/Index.cshtml">Databases</a><br /><br />
                <a class="navlinkB" href="./Project1.cshtml">Oracle</a><br /><br />
                <a class="navlinkB" href="./Project2.cshtml">Derby</a><br /><br />
                <a class="navlinkB" href="./Project3.cshtml">MySQL</a><br /><br />
                <a class="navlinkB" href="./Project4.cshtml">SQL Server</a><br /><br />
                <a class="navlinkB" href="./Project5.cshtml">PostgreSQL</a><br /><br />
            </div>
            <div id="idCenterRowMain">
                <h2>
                    @Page.Title
                </h2>
                @RenderBody()
            </div>
            <div id="idCenterRowRight">
                <h4>
                    Information
                </h4>
                This is written with ASP.NET as a Web Page Application.<br />
                Other versions can be found here:<br />
                @Html.Raw(@Page.Versions)
            </div>
        </div>
        <Footer id="idFooterRow">
            <div id="idFooterMain">
                <div id="idFooterContent">
                    © Copyright 2012 All rights reserved<br>
                    House That Kamurai Built<br>
                </div>
                <div id="idFooterManagement">
                    Website managed by Kamurai.
                </div>
            </div>
        </Footer>
    </div>
</body>
</html>